<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Define your shape -->
    <item android:id="@+id/circle_shape">
        <shape android:shape="oval">
            <solid android:color="#00B5AE" />
            <size android:height="134dp" android:width="134dp"/>
        </shape>
    </item>

    <!-- Add your bitmap image -->
    <item android:drawable="@drawable/a21"
        android:gravity="center"
        android:width="100dp"
        android:height="100dp"/>
</layer-list>